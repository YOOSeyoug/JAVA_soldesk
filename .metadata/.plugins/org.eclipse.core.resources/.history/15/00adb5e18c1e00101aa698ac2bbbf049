package New_Test.Machine;

public class Drink {
	
	public String name;
	int price;
	
	public Drink() {
		// TODO Auto-generated constructor stub
	}
	
	private Drink(String name,int price) {
		this.name=name;
		this.price=price;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	

}
