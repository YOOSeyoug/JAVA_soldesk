package chapter11;

public class FunInterMain_07 {
	
	public void test() {
		
		//익명의 클래스
		new FunInter() { //FunInter funinter = new funInter
			
			@Override
			public void printData() {
				System.out.println("목요일!!");
				
			}
		};
	}
	

	public static void main(String[] args) {
		FunInterMain_07 funinter = new FunInterMain_07();
		funinter.test();

	}

}
