package chapter06;

public class Example_person_Main {

	public static void main(String[] args) {
		
		Example_person inbody = new Example_person();
		
		inbody.name = "홍길동";
		inbody.height = 180;
		inbody.weight = 80;
		
		System.out.println(inbody.name+"님의 키는 "+inbody.height+"cm이고, 몸무게는 "+inbody.weight+"kg 입니다.");

	}

}
