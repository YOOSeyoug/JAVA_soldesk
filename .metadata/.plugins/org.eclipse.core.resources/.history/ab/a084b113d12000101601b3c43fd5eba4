package chapter12.string;

public class StringMain_01 {

	public static void main(String[] args) {
		
		//String -> 불변 객체(Immutable Object)
		
		String str="";
		String str1="Hi everybody!";
		String str2=" Have a nice Day!";
		//str1 주소 같음
		System.out.println(System.identityHashCode(str1)); //331510866
		//st1 주소 같음
		str=str1+str2;
		System.out.println(System.identityHashCode(str1)); //331510866
		System.out.println(str);
		
		//str1.concat() 컴파일시 새로운 객체로 문자열이 반환됨
		str1=str1.concat(str2);
		System.out.println(System.identityHashCode(str1)); //640363654
		
		//str1
		str = str1.concat(str2);
		System.out.println(System.identityHashCode(str1));
		
		
		

	}

}
