package chapter09;

public class AnimalMain {

	public static void main(String[] args) {
		Dog dog = new Dog();
		Dog.sound2();
		Dog.age = 10;
		animalSound(dog);
		
		animalSound(new Cat());
		
		
	}
	
	public static void animalSound(Animal animal) {
		animal.sound();
		animal.breathe();
		System.out.println(Dog.age);
	}
	
	

}
