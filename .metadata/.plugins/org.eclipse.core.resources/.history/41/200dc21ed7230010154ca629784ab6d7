	public void personMgr() {
		
		int select;
		Person p;
		
		//Person객체들만 저장할 ArrayList 생성
		ArrayList<Person> personarr = new ArrayList<Person>();
		
		while(true) {
			System.out.println("(1)회원가입, (2)정보삭제, (3)정보검색, (4)종료");
			
			Scanner scan = new Scanner(System.in);
			select = scan.nextInt();
			
			switch (select) {
			case value: {
				
				yield type;
			}
			default:
				System.out.println("프로그램 종료");
				return;
			}
			
		}//while
		
		
	}//메서드
	
	
	
	
