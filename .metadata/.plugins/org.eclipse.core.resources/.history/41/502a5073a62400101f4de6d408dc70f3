package chapter14.queue;

import java.util.LinkedList;
import java.util.Queue;

public class MessageMain_02 {

	public static void main(String[] args) {
		
		Queue<Message> messageQueue = new LinkedList<Message>();
		
		Message lee = new Message("sendMail", "ÀÌÈ£ÁØ");
		messageQueue.offer(lee);
		messageQueue.offer(new Message("sendSNS", "±èÁö¿µ"));
		messageQueue.offer(new Message("sendKakaotalk", "±è¹ÎÁÖ"));
		
		while(!messageQueue.isEmpty()) {
			Message message = messageQueue.poll();
			
			switch (message.command) {
			case "sendMail": {
				System.out.println(message.to+"´Ô¿¡°Ô ¸ŞÀÏÀ» º¸³Â½À´Ï´Ù.");
				break;
			}
			
			case "sendSNS": {
				System.out.println(message.to+"´Ô¿¡°Ô SNS¸¦ º¸³Â½À´Ï´Ù.");
				break;
			}
			
			case "sendKakaotalk": {
				System.out.println(message.to+"´Ô¿¡°Ô Ä«Ä«¿ÀÅåÀ» º¸³Â½À´Ï´Ù.");
				break;
			}
			}//switch
		}//while
		
		
		

	}

}
