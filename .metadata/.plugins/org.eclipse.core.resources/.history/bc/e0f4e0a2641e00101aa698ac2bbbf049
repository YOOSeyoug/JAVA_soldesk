package chapter06;

public class Example_person_Main {

	public static void main(String[] args) {
		
		Example_person body = new Example_person();
		
		body.name = "홍길동";
		body.height = 180;
		body.weight = 80;
		
		System.out.println(body.name+"님의 키는 "+body.height+"cm이고, 몸무게는 "+body.weight+"kg 입니다.");

	}

}
