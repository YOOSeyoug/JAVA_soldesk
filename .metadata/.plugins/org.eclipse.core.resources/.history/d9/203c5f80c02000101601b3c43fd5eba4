package chapter12.wrapper;

public class ValueCompareMain_04 {

	public static void main(String[] args) {
		// 오토박싱의 범위
		System.out.println("[-128 ~ 128초과할 경우]"); //1byte //1byte = 8bit, int 4byte, double 8byte
		Integer obj1 = 300; //4byte = 32bit
		Integer obj2 = 300; 
		
		//주소비교
		System.out.println("==결과: "+(obj1==obj2)); //false, 객체다 보니 각각의 주소가 다름
		System.out.println("==언박싱한 후 결과: "+(obj1.intValue()==obj2.intValue())); //false, 객체다 보니 각각의 주소가 다름
		
	}

}
