package chapter06;

public class TeamGroup {
	
	//멤버변수
	private TeamMember_05[] member=new TeamMember_05[5];
	
	//기본생성자
	
	//초기화 메서드
	public void init() {
		
		//생성ㅇ자 오버로드 사용
		member[0]=new TeamMember_05("신희우","남학생"); //기본 생성자를 만들어서 공간을 만든 것
		member[1]=new TeamMember_05("최대호","남학생");
		member[2]=new TeamMember_05("박현규","남학생");
		member[3]=new TeamMember_05("윤준영","남학생");
		member[4]=new TeamMember_05("유세영","여학생");
		
	}

}
